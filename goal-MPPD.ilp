
#program check(t).

processed(O,A,t) :- ordered(O,A), target(O,P), shelved(S,A), serves(R,P,t), carries(R,S,t). % No resources
processed(O,A,t) :- processed(O,A,t-1).

processed(O,  t) :- isOrder(O), processed(O,A,t) : ordered(O,A).                            % No ordering

:- not processed(O,t), isOrder(O), query(t).

:- carries(R,_,t),                   isRobot(R), query(t).
:- position(S,C), position(_,C,t), isStation(S), query(t).
