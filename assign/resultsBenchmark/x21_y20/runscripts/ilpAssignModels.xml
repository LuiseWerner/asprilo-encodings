<runscript output="output_ilpAssignModels">

	<machine name="zuse" cpu="24x8xE5520@2.27GHz" memory="24GB"/>
	
	<config name="seq-generic" template="templates/seq-generic.sh"/>
	<system name="clingo" version="ilpAssignModels" measures="clingo" config="seq-generic">
		<setting name="setting-1" cmdline="'--stats 1 --out-atomf=%s.'" tag="basic" ppn="8" procs="1" pbstemplate="templates/single.pbs"/>
	</system>
	
	<pbsjob name="seq-generic" timeout="3600" runs="1" script_mode="timeout" walltime="01:00:00" cpt="4" partition="short"/>

	<benchmark name="x3">
		<folder path="benchmarks/assign/assigned"/>
	</benchmark>

	<project name="x3" job="seq-generic">
		<runtag machine="zuse" benchmark="x3" tag="basic"/>
	</project>

</runscript>
